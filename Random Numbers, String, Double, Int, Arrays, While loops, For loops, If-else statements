
//13.6Exercise#5RandCreate.ch
//declare
int number, number1, number2;
//initialize
number = randint(1, 100);
number1 = randint(1, 100);
number2 = randint(1, 100);
printf("The three random numbers are %d, %d, and %d\n", number, number1, number2);
scanf("%d", &number);
scanf("%d", &number1);
scanf("%d", &number2);

//13.6#7mulnumtest.ch
//declare
int Fnum, Snum;
double SumNumber, SumAnswer;
//initialize
Fnum = randint(1, 10);
Snum = randint(1, 10);
printf("The two numbers are %d and %d\n", Fnum, Snum);
printf("What is the Sum of the two numbers?\n");
scanf("%lf", &SumAnswer);
SumNumber = Fnum+Snum;

if(SumNumber==SumAnswer){
	printf("Correct\n");
}else{
printf("Incorrect, the answer is %.lf\n",SumNumber);
}

#include <chplot.h>
